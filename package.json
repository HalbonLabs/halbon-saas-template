{
  "name": "halbon-saas-template",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.15.9",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:strict": "eslint . --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "typecheck:strict": "tsc --noEmit -p tsconfig.strictest.json",
    "prebuild": "prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "postinstall": "prisma generate",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@prisma/client": "5.16.2",
    "next": "14.2.31",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@types/node": "22.17.2",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.30",
    "prettier": "3.6.2",
    "prisma": "5.16.2",
    "typescript": "5.9.2",
    "@vitest/coverage-v8": "3.2.4",
    "happy-dom": "18.0.1",
    "vitest": "3.2.4"
  },
  "pnpm": {
    "overrides": {
  "@types/json5": "2.2.0",
  "json5": "2.2.3"
    }
  }
}