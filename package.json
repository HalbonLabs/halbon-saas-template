{
  "name": "halbon-saas-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.14.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:strict": "eslint . --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "typecheck:strict": "tsc --noEmit -p tsconfig.strictest.json",
    "prebuild": "prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "postinstall": "prisma generate",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@prisma/client": "6.14.0",
    "next": "15.4.6",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "zod": "4.0.17"
  },
  "devDependencies": {
    "@eslint/js": "9.33.0",
    "@next/eslint-plugin-next": "15.4.6",
    "@types/node": "22.17.2",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "9.33.0",
    "eslint-config-next": "15.4.6",
    "globals": "^16.0.0",
    "happy-dom": "18.0.1",
    "prettier": "3.6.2",
    "prisma": "6.14.0",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  },
  "pnpm": {
    "overrides": {
      "@types/json5": "2.2.0",
      "json5": "2.2.3"
    }
  }
}